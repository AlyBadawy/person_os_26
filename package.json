{
  "name": "app",
  "private": true,
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "esbuild": "^0.27.0",
    "typescript": "^5.6.3",
    "eslint": "^8.52.0",
    "@typescript-eslint/parser": "^6.8.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4"
  },
  "scripts": {
    "build": "esbuild app/javascript/application.js app/javascript/react-dashboard.tsx --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets --loader:.js=jsx --loader:.tsx=tsx --loader:.ts=ts",
    "build:css": "npx @tailwindcss/cli -i ./app/assets/stylesheets/application.tailwind.css -o ./app/assets/builds/application.css --minify",
    "lint": "eslint 'app/javascript/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'app/javascript/**/*.{js,jsx,ts,tsx}' --fix"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.20",
    "@tailwindcss/cli": "^4.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^4.1.17"
  }
}
